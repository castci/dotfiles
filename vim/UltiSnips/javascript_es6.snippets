# Generator function
snippet gfn "Generator function fun*"
function* ${1:name}(${2}) {
  yield ${3};
}
endsnippet

snippet af "Arrow function" 
(${1}) => {
  ${VISUAL}$0
}
endsnippet

snippet class "class"
class ${1:name} {
  constructor(${2:arg}) {
      ${3:// init}
  }
  ${4}
}
endsnippet

snippet forof "forof"
for (let ${1:s} of ${2:sequence}) {
  ${3}
}
endsnippet

snippet im "import"
import ${1:foo} from '${2:bar}';
endsnippet

snippet ex "export"
export ${1:default} ${2:bar}
endsnippet
